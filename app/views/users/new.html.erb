<h1>Sign Up!</h1>

<ul>
<%= form_for @user do |f| %>
  <%= f.label :name %>
  <br>
  <%= f.text_field :name %>
  <br></br>

  <%= f.label :password %>
  <br>
  <%= f.password_field :password %>
  <br></br>

  <label>Height in inches</label>
  <br>
  <%= f.text_field :height %>
  <br></br>

  <%= f.label :happiness %>
  <br>
  <%= f.text_field :happiness %>
  <br></br>

  <%= f.label :nausea %>
  <br>
  <%= f.text_field :nausea %>
  <br></br>

  <label>How much money can you exchange for William Bucks!</label>
  <br>
  <%= f.text_field :tickets %>
  <br></br>

  <label>Admin?</label>
  <%= f.check_box :admin %>


  <br></br>
  <%= f.submit "Create User" %>
<% end %>
</ul>